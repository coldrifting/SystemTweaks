  "FileTypeActions": {
    "Run": {
      "Name": "open",
      "Label": "Run",
      "Command": "\"%1\" %*"
    },
    "RunAs": {
      "Name": "Runas",
      "Label": "Run (Admin)",
      "Command": "\"%1\" %*",
      "HasShieldIcon": true
    },
    "RunPowershell": {
      "Name": "0",
      "Label": "Run",
      "Command": "\"%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -ExecutionPolicy Bypass \"%1\""
    },
    "RunAsPowershell": {
      "Name": "Runas",
      "Label": "Run (Admin)",
      "Command": "\"%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -ExecutionPolicy Bypass \"%1\"",
      "HasShieldIcon": true
    },
    "InstallINF": {
      "Name": "install",
      "Label": "Install",
      "Command": "\"%SystemRoot%\\System32\\InfDefaultInstall.exe\" \"%1\""
    },
    "EditText": {
      "Name": "z_edit",
      "Label": "Edit",
      "Command": "\"$(TextEditor)\" \"%1\""
    },
    "EditResource": {
      "Name": "z_edit",
      "Label": "Edit",
      "Command": "\"$(ResourceEditor)\" \"%1\""
    },
    "OpenArchive": {
      "Name": "open",
      "Label": "Open",
      "Command": "\"$(ArchiveViewer)\" \"%1\""
    },
    "ExtractArchive": {
      "Name": "extract",
      "Label": "Extract",
      "Command": "\"$(ArchiveExtractor)\" x -o* \"%1\""
    },
    "ExtractArchiveHere": {
      "Name": "extractHere",
      "Label": "Extract Here",
      "Command": "\"$(ArchiveExtractor)\" x \"%1\""
    },
    "PlayAudio": {
      "Name": "play",
      "Label": "Play",
      "Command": "\"$(AudioPlayer)\" \"%1\""
    },
    "EditAudio": {
      "Name": "edit",
      "Label": "Edit",
      "Command": "\"$(AudioEditor)\" \"%1\""
    },
    "OpenImage": {
      "Name": "open",
      "Label": "Open",
      "Command": "\"$(ImageViewer)\" \"%1\""
    },
    "EditImage": {
      "Name": "r_edit",
      "Label": "Edit",
      "Command": "\"$(ImageEditor)\" \"%1\""
    },
    "PlayVideo": {
      "Name": "play",
      "Label": "Play",
      "Command": "\"$(VideoPlayer)\" \"%1\""
    },
    "RunJar": {
      "Name": "OpenJar",
      "Label": "Run",
      "Command": "\"$(Java)\" -jar \"%1\""
    },
    "PreviewFont": {
      "Name": "preview",
      "Label": "Preview",
      "Command": "\"%SystemRoot%\\System32\\fontview.exe\" \"%1\""
    },
    "EditMesh": {
      "Name": "z_edit_mesh",
      "Label": "Edit",
      "Command": "\"$(3dEditor)\" --python \"C:\\Config\\Scripts\\blender_auto_open.py\" -- \"%1\""
    },
    "OpenPDF": {
      "Name": "open",
      "Label": "Open",
      "Command": "\"$(PdfViewer)\" \"%1\""
    },
    "RunPython": {
      "Name": "Run",
      "Label": "Run",
      "Command": "\"$(python)\" \"%1\""
    },
    "RepairMsi": {
      "Name": "Repair",
      "Label": "Re&pair",
      "Command": "\"%SystemRoot%\\System32\\msiexec.exe\" /f \"%1\" %*"
    },
    "UninstallMsi": {
      "Name": "Uninstall",
      "Label": "&Uninstall",
      "Command": "\"%SystemRoot%\\System32\\msiexec.exe\" /x \"%1\" %*"
    }
  },
